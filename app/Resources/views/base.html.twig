<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}
                <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
               <link rel="stylesheet" href="{{ asset('component/materialize/css/materialize.min.css')}}"/>
        {% endblock %}
        {% block javascripts %}
            <script src="{{asset('component/materialize/js/materialize.min.js')}}"></script>
           <script src="{{asset('component/materialize/js/materialize.js')}}"></script>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <style>
            .center-content{
                text-align: center;
            }
            
            .tomato{
                background-color: tomato;
            }
        </style>
    </head>
    <body>
        {% block body %}{% endblock %}
        
    </body>
</html>
